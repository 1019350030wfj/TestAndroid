<?xml version="1.0" encoding="utf-8"?>
<com.jayden.testandroid.customview.dragviewhelper.JaydenDragLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/drawerlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/ll_group"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/base_gray_green"
        android:orientation="vertical">

        <com.jayden.testandroid.customview.MyWebView
            android:id="@+id/webview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="200dp"
        android:layout_height="560dp"
        android:background="@drawable/wanyongbiao"/>
</com.jayden.testandroid.customview.dragviewhelper.JaydenDragLayout>